import "operations.eol";
rule UseCase2Model
    transform uc : UML!UseCase
    to m : FUML!Model {
    	
    	var packages = m.getPackagedElements();
		
		m.getPackagedElements().addAll(UML!Model.all.first.getPackagedElements());
		"DeleteElements".println();
    	for (p: UML!Package in m.getPackagedElements()) {
    		p.annotateIgnore(uc);
    	}
    	System.context.transformationTrace.println();
    
	}
	